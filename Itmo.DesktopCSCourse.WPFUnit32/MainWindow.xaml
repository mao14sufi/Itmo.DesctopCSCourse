<Window x:Class="Itmo.DesktopCSCourse.WPFUnit32.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Itmo.DesktopCSCourse.WPFUnit32"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtBox" HorizontalAlignment="Left" Margin="44,176,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <StackPanel ButtonBase.Click="StackPanel_Click_1"  Margin="228,0,0,0">
            <Button x:Name="butAdd" Content="x+x"/>
            <Button x:Name="butMult" Content="x*x"/>
            <Button x:Name="butSqrt" Content="sqrt(x)"/>
        </StackPanel>


    </Grid>
</Window>
